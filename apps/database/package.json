{
    "name": "@foresee/database",
    "version": "1.0.0",
    "description": "Database services for Foresee",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./generated": {
            "types": "./generated/index.d.ts",
            "import": "./generated/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "generate": "prisma generate",
        "migrate": "prisma migrate dev",
        "migrate:deploy": "prisma migrate deploy",
        "push": "prisma db push",
        "studio": "prisma studio",
        "seed": "prisma db seed",
        "verify": "npx tsx src/verify-prisma.ts"
    },
    "dependencies": {
        "@prisma/client": "^6.19.2"
    },
    "devDependencies": {
        "prisma": "^6.19.2",
        "tsx": "^4.21.0",
        "typescript": "^5.8.3"
    },
    "peerDependencies": {
        "@prisma/client": "^6.19.2"
    }
}